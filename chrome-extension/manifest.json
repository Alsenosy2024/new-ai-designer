{
  "manifest_version": 3,
  "name": "AI Architect Designer",
  "version": "1.0.0",
  "description": "AI-powered architectural design system with multi-agent coordination for real building design generation",
  "author": "AI Designer Team",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "AI Architect Designer"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:8001/*",
    "http://127.0.0.1:8001/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "options_page": "options.html"
}
