{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/evaluate.html"
    }
  ],
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
